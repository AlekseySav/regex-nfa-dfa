/$

/ input is dfa already
$ tools/input "%{in-1}" | ./1nfa-dfa | ./cmp-nfa "$(tools/input \"%{in-1}\")"
$ tools/input "%{in-2}" | ./1nfa-dfa | ./cmp-nfa "$(tools/input \"%{in-2}\")"
$ tools/input "%{in-3}" | ./1nfa-dfa | ./cmp-nfa "$(tools/input \"%{in-3}\")"
$ tools/input "%{in-4}" | ./1nfa-dfa | ./cmp-nfa "$(tools/input \"%{in-4}\")"
$ tools/input "%{in-5}" | ./1nfa-dfa | ./cmp-nfa "$(tools/input \"%{in-5}\")"

/ check if we at least built DFA
$ tools/input "%{in-6}" | ./1nfa-dfa | ./is dfa
$ tools/input "%{in-7}" | ./1nfa-dfa | ./is dfa
$ tools/input "%{in-8}" | ./1nfa-dfa | ./is dfa
$ tools/input "%{in-9}" | ./1nfa-dfa | ./is dfa

/ check if dfa is correct
$ tools/input "%{in-6}" | ./1nfa-dfa | ./run "%{str-6}" | tools/cmp "$(tools/input \"%{in-6}\" | ./run \"%{str-6}\")"
$ tools/input "%{in-7}" | ./1nfa-dfa | ./run "%{str-7}" | tools/cmp ";ab;aba;abab;ababa;ababab;"
$ tools/input "%{in-8}" | ./1nfa-dfa | ./run "%{str-8}" | tools/cmp "abab;aba;abababa;abab;aba;a;"
$ tools/input "%{in-9}" | ./1nfa-dfa | ./run "%{str-9}" | tools/cmp "abc;"

