$ tools/input "%{in-4}" | ./cdfa-re | ./re-nfa | ./nfa-1nfa | ./1nfa-dfa | ./dfa-cdfa | ./cdfa-min | ./cmp-nfa "$(tools/input \"%{in-4}\" | ./cdfa-min)"
